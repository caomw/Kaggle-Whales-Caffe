# The train/test net protocol buffer definition
net: "train/point2_train.prototxt"
#test_state: { stage: 'test-on-train' }
#test_iter: 600
#test_interval: 600
#test_compute_loss: true
type: "AdaDelta"
gamma: 0.0001
base_lr: 1.0
momentum: 0.95
delta: 1e-6
weight_decay: 0.0005
power: 0.75
lr_policy: "fixed"
display: 4544
max_iter: 204480
snapshot_prefix: "model_2"
solver_mode: GPU
