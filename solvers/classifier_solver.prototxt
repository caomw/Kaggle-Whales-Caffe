# The train/test net protocol buffer definition
net: "train/classifier.prototxt"
test_state: { stage: 'test-on-train' }
test_iter: 10
test_interval: 100
type: "RMSProp"
rms_decay: 0.98
test_compute_loss: true
gamma: 0.0001
base_lr: 0.01
momentum: 0.0
#delta: 1e-8
weight_decay: 0.0005
power: 0.75
lr_policy: "inv"
display: 20
max_iter: 6000
snapshot_prefix: "classifier"
solver_mode: GPU
